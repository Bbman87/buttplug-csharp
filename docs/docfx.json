{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Buttplug/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
	          "gh-pages/**"
	        ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "src": ".."
      },
      {
        "files": "*.md",
        "src": ".."
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "gh-pages/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "gh-pages/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "gh-pages/**"
        ]
      }
    ],
    "dest": "gh-pages",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true,
    "xref": [
      "https://docs.microsoft.com/dotnet/xrefmap.yml"
    ]
  }
}
